{
  "expo": {
    "name": "hotel-management-app",
    "slug": "hotel-management-app",
    "version": "1.0.0",
    "sdkVersion": "50.0.0",
    "platforms": ["ios", "android", "web"],
    "android": {
      "package": "com.tarinxyz.hotelmanagementapp"
    },
    "extra": {
      "eas": {
        "projectId": "0d17d25c-adb7-4c90-88c9-01295ac7af8e"
      },
      "developerConfig": {
        "disableYellowBox": true,
        "ignoredWarnings": ["Warning: ", "Require cycle:"]
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "./scripts/remove-console-logs.js"
        }
      ]
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"]
  }
}
